{
  "name": "@osag/pyrene-graphs",
  "version": "1.1.10",
  "description": "Open Systems Graphs Component Library",
  "main": "dist/pyrene-graphs.js",
  "style": "dist/pyrene-graphs.css",
  "repository": "https://github.com/open-ch/pyrene.git",
